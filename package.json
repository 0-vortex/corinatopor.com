{
  "name": "corinatopor.com",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "author": "TED Vortex <ted.vortex@gmail.com>",
  "contributors": [
    "TED Vortex <ted.vortex@gmail.com>",
    "Robert Bratucu <robert.bratucu@gmail.com>",
    "Eduard-Constantin Stoica <eduard.stoica@student.manchester.ac.uk>"
  ],
  "engines": {
    "node": ">=14.16.1",
    "npm": ">=7.4.2"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "fontsource-esteban": "^4.0.0",
    "fontsource-montserrat": "^4.0.0",
    "gatsby": "^3.6.2",
    "gatsby-image": "^3.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-remark-images": "^5.3.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-share": "^4.4.0",
    "sass": "^1.34.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "^1.13.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --verbose",
    "format": "eslint . --fix --report-unused-disable-directives",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "push": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/0-vortex/corinatopor.com.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
